{
  "name": "@gustavojosemelo/n8n-nodes-bitrix24",
  "version": "0.0.8",
  "description": "n8n community node for Bitrix24 \u2014 CRM, Tasks, Open Channels, Chatbots, Drive, Document Generator, News Feed and Raw API",
  "keywords": [
    "n8n-community-node-package",
    "bitrix24",
    "crm",
    "tasks",
    "automation",
    "open-channels",
    "chatbot",
    "drive",
    "document-generator",
    "news-feed"
  ],
  "license": "MIT",
  "homepage": "https://github.com/gustavojosemelo/n8n-nodes-bitrix24",
  "bugs": {
    "url": "https://github.com/gustavojosemelo/n8n-nodes-bitrix24/issues"
  },
  "author": {
    "name": "Gustavo Jos\u00e9 Melo",
    "email": "gustavo@universonext.com.br"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gustavojosemelo/n8n-nodes-bitrix24.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/nodes/Bitrix24/credentials/Bitrix24Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bitrix24/Bitrix24.node.js",
      "dist/nodes/Bitrix24/Bitrix24Trigger.node.js"
